# Firebase Configuration - Copy to .env and fill with actual values
# IMPORTANT: Never commit .env file to version control

# Firebase Project Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789012
VITE_FIREBASE_APP_ID=1:123456789012:web:abcdef123456789
VITE_FIREBASE_MEASUREMENT_ID=G-ABCDEFGHIJK

# Exchange Rate API Configuration (Optional)
VITE_EXCHANGE_RATE_API_URL=https://api.exchangerate-api.com/v4/latest/INR
VITE_EXCHANGE_RATE_API_KEY=your_api_key_here
VITE_FALLBACK_API_URL=https://api.exchangerate-api.com/v4/latest/INR

# Application Configuration
VITE_USE_STATIC_RATES=false
VITE_EXCHANGE_RATE_CACHE_DURATION=1800000  njjv